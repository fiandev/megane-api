{
  "buildCommand": "pip install -r requirements.txt && flask run",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "redirects": [
    {
      "source": "/docs/<filename>.md",
      "destination": "/static/docs/$filename.md",
      "permanent": true
    }
  ]
}